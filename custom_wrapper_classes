public class Coding {
	private int i;

	Coding() {
	}

	Coding(int i) {
		this.i = i;
	}

	public int getValue() {
		return i;
	}

	public void setValue(int i) {
		this.i = i;
	}

	public String toString() {
		return Integer.toString(i);
	}
}

public class TestCoding {
	public static void main(String[] args) {
		Coding c = new Coding(45);
		System.out.println(c);
	}
}
