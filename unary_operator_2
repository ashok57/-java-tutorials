public class OperatorExample{
    public static void main(String args[] ){
        int x = 10;
        int y = -10;
        boolean c= true;
        boolean d  = false;
        System.out.println(~x);
        System.out.println(~y);
        System.out.println(!c);
        System.out.println(!d);
       
    }
}
