class TestArray {
	public static void main(String[] args) {
		int arr[][] = { { 1, 2, 3 }, { 2, 6, 9 } };
		int ary[][] = { { 1, 6, 8 }, { 9, 6, 3 } };
		int arz[][] = new int[2][3];
		for (int i = 0; i < 2; i++) {
			for (int j = 0; j < 3; j++) {
				arz[i][j] = arr[i][j] + ary[i][j];
				System.out.print(arz[i][j] + " ");
			}
			System.out.println();
		}

	}
}
