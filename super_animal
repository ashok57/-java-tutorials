class Animal {
	String color = "white";
}

class Dog extends Animal {
	String color = "black";

	void printColor() {
		System.out.println(color);// prints the color of the dog
		System.out.println(super.color); // prints the color of the animal class
	}
}

class Test {
	public static void main(String[] args) {
		Dog d = new Dog();
		d.printColor();
	}
}
